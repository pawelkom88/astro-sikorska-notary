---
import Breadcrumbs from "../components/breadcrumbs/Breadcrumbs.astro";
import ContactSection from "../components/contact/Contact.astro";
import { routes } from "../components/header/Header.astro";
import Layout from "../layouts/Layout.astro";
---

<Layout
  title="Mobile Notary Service in Bristol - Contact Sikorska Notary"
  description="Reach out to Sikorska Notary for a flexible and reliable mobile notary service in Bristol, available during weekends and out of office hours. Contact us for a personalized quote tailored to your specific notarial needs."
  image="https://www.sikorskanotary.co.uk/Sikorska-Notary-Office.ca8207a8.webp">
  <ContactSection />
  <Breadcrumbs
    links={[
      { name: "Blog", href: routes.blog },
      { name: "Homepage", href: routes.homePage },
    ]}
    current="Contact"
  />
</Layout>
